{
  "name": "floorplan3d.react.spa",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "clean": "rimraf public/_framework public/Content public/assets.js public/evergine.js",
    "compile": "npm run evergine:copy --configuration=%npm_config_configuration%",
    "evergine:copy": "npm run evergine:copy:wasm --configuration=%npm_config_configuration% && npm run evergine:copy:assets --configuration=%npm_config_configuration% && npm run evergine:copy:content --configuration=%npm_config_configuration% && npm run evergine:copy:evergine --configuration=%npm_config_configuration%",
    "evergine:copy:wasm": "ncp ../Floorplan3D.WebReact/bin/%npm_config_configuration%/net8.0/wwwroot/_framework public/_framework",
    "evergine:copy:assets": "ncp ../Floorplan3D.WebReact/bin/%npm_config_configuration%/net8.0/wwwroot/assets.js public/assets.js",
    "evergine:copy:content": "ncp ../Floorplan3D.WebReact/bin/%npm_config_configuration%/net8.0/wwwroot/Content public/Content",
    "evergine:copy:evergine": "ncp ../Floorplan3D.WebReact/js/evergine.js public/evergine.js"
  },
  "dependencies": {
    "evergine-react": "^1.0.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "ncp": "^2.0.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  }
}
